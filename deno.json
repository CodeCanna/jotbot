{
  "tasks": {
    "dev": "deno run --allow-write --allow-read --allow-net --allow-env --watch main.ts",
    "test": "deno test --allow-write --allow-read --allow-net --allow-env ./tests/*",
    "bin": "deno compile --allow-write --allow-net --allow-env --allow-read -o ./bin/JotBot main.ts",
    "bin-arm": "deno compile --allow-write --allow-net --allow-env --allow-read --target aarch64-unknown-linux-gnu -o ./bin/JotBotArm main.ts"
  },
  "imports": {
    "@grammyjs/commands": "npm:@grammyjs/commands@^1.2.0",
    "@grammyjs/conversations": "npm:@grammyjs/conversations@^2.1.1",
    "@grammyjs/files": "npm:@grammyjs/files@^1.2.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "grammy": "npm:grammy@^1.38.4"
  }
}
